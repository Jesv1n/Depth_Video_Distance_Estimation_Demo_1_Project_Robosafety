b = new Blob(buffers, {type: 'application/binary'});\n","      const a = document.createElement('a');\n","      a.href = window.URL.createObjectURL(blob);\n","      a.download = filename;\n","      div.appendChild(a);\n","      a.click();\n","      div.remove();\n","    }\n","  "]},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<IPython.core.display.Javascript object>"],"application/javascript":["download(\"download_37ea9d26-d483-4029-85f2-e06686997937\", \"bottle_cup_distance_yolov8.mp4\", 184653681)"]},"metadata":{}}]}],"metadata":{"colab":{"provenance":[{"file_id":"1XQQRrldhQ8Jn_ra9UGQxsrluAg-gcyDU","timestamp":1753639276903},{"file_id":"/v2/external/notebooks/intro.ipynb","timestamp":1753639257385}],"gpuType":"T4"},"kernelspec":{"display_name":"Python 3","name":"python3"},"accelerator":"GPU"},"nbformat":4,"nbformat_minor":0}