{"cells":[{"cell_type":"code","source":["# Mount Google Drive\n","from google.colab import drive\n","drive.mount('/content/drive')\n","print(\"Google Drive mounted successfully!\")\n","\n","# Install Ultralytics  and other necessary libraries\n","!pip install -q ultralytics opencv-python matplotlib\n","print(\"Required libraries installed.\")"],"metadata":{"id":"AtoFH4xQg24F","executionInfo":{"status":"ok","timestamp":1753633376180,"user_tz":-120,"elapsed":7238,"user":{"displayName":"","userId":""}},"outputId":"a445843e-0e07-40a2-8a13-78db87cce03b","colab":{"base_uri":"https://localhost:8080/"}},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Drive already mounted at /content/drive; to attempt to forcibly remount, call drive.mount(\"/content/drive\", force_remount=True).\n","Google Drive mounted successfully!\n","Required libraries installed.\n"]}]},{"cell_type":"code","source":["import cv2\n","import torch\n","from ultralytics import YOLO\n","import numpy as np\n","import os\n","import time\n","from google.colab import files\n","\n","RGB_VIDEO_DRIVE_PATH = \"/content/drive/MyDrive/Depth_Video_Distance_Estimation/RobotTest1.mp4\"\n","DEPTH_VIDEO_DRIVE_PATH = \"/content/drive/MyDrive/Depth_Video_Distance_Estimation/estimated_depth_video1.mp4\"\n","OUTPUT_VIDEO_FILENAME = \"bottle_cup_distance_yolov8.mp4\"\n","\n","YOLO_MODEL_NAME = \"yolov8x.pt\"\n","\n","CONFIDENCE_THRESHOLD = 0.5\n","TARGET_CLASS_NAMES = [\"bottle\", \"cup\"]  # Detecting bottle and cup\n","\n","APPROX_INTRINSICS = {\n","    'fx': 1200.0,\n","    'fy': 1200.0,\n","    'cx': 960.0,\n","    'cy': 540.0\n","}\n","\n","# --- Check input video existence ---\n","if not os.path.exists(RGB_VIDEO_DRIVE_PATH):\n","    raise FileNotFoundError(f\"RGB video '{RGB_VIDEO_DRIVE_PATH}' not found.\")\n","if not os.path.exists(DEPTH_VIDEO_DRIVE_PATH):\n","    raise FileNotFoundError(f\"Depth video '{DEPTH_VIDEO_DRIVE_PATH}' not found.\")\n","\n","# --- Load YOLOv8 model ---\n","print(f\"Loading YOLO model: {YOLO_MODEL_NAME}...\")\n","model = YOLO(YOLO_MODEL_NAME)\n","all_class_names = model.names\n","\n","# Get class IDs for target objects\n","target_class_ids = [cls_id for cls_id, name in all_class_names.items() if name in TARGET_CLASS_NAMES]\n","\n","print(f\"Target class IDs: {target_class_ids}\")\n","\n","# --- Video Setup ---\n","cap_rgb = cv2.VideoCapture(RGB_VIDEO_DRIVE_PATH)\n","cap_depth = cv2.VideoCapture(DEPTH_VIDEO_DRIVE_PATH)\n","\n","frame_width = int(cap_rgb.get(cv2.CAP_PROP_FRAME_WIDTH))\n","frame_height = int(cap_rgb.get(cv2.CAP_PROP_FRAME_HEIGHT))\n","fps = cap_rgb.get(cv2.CAP_PROP_FPS)\n","num_frames = int(min(cap_rgb.get(cv2.CAP_PROP_FRAME_COUNT), cap_depth.get(cv2.CAP_PROP_FRAME_COUNT)))\n","\n","APPROX_INTRINSICS['cx'] = frame_width / 2.0\n","APPROX_INTRINSICS['cy'] = frame_height / 2.0\n","\n","out_video = cv2.VideoWriter(OUTPUT_VIDEO_FILENAME, cv2.VideoWriter_fourcc(*'mp4v'), fps, (frame_width, frame_height))\n","\n","# --- Helper Functions ---\n","def get_3d_point(u, v, depth_frame, intrinsics):\n","    fx, fy = intrinsics['fx'], intrinsics['fy']\n","    cx, cy = intrinsics['cx'], intrinsics['cy']\n","    v = np.clip(v, 0, depth_frame.shape[0] - 1)\n","    u = np.clip(u, 0, depth_frame.shape[1] - 1)\n","    depth_mm = depth_frame[int(v), int(u)]\n","    if depth_mm == 0:\n","        return None\n","    Z = depth_mm / 1000.0\n","    X = (u - cx) * Z / fx\n","    Y = (v - cy) * Z / fy\n","    return np.array([X, Y, Z])\n","\n","def calculate_euclidean_distance(p1, p2):\n","    return np.linalg.norm(p1 - p2) if p1 is not None and p2 is not None else None\n","\n","# --- Main Processing Loop ---\n","frame_count = 0\n","while frame_count < num_frames:\n","    ret_rgb, frame_rgb = cap_rgb.read()\n","    ret_depth, frame_depth = cap_depth.read()\n","    if not ret_rgb or not ret_depth:\n","        break\n","\n","    if len(frame_depth.shape) == 3:\n","        gray = cv2.cvtColor(frame_depth, cv2.COLOR_BGR2GRAY)\n","        max_depth_m = 5.0\n","        min_depth_m = 0.5\n","        frame_depth_mm = (min_depth_m * 1000) + (255 - gray.astype(np.float32)) * ((max_depth_m * 1000 - min_depth_m * 1000) / 255.0)\n","        frame_depth_mm = frame_depth_mm.astype(np.uint16)\n","    else:\n","        frame_depth_mm = frame_depth\n","\n","    results = model.predict(frame_rgb, conf=CONFIDENCE_THRESHOLD, classes=target_class_ids, verbose=False)\n","    annotated_frame = frame_rgb.copy()\n","    detected_objects = []\n","\n","    for r in results:\n","        for det in r.boxes:\n","            x1, y1, x2, y2 = map(int, det.xyxy[0])\n","            conf = float(det.conf[0])\n","            cls_id = int(det.cls[0])\n","            class_name = all_class_names[cls_id]\n","            u_center = (x1 + x2) // 2\n","            v_center = (y1 + y2) // 2\n","            point3d = get_3d_point(u_center, v_center, frame_depth_mm, APPROX_INTRINSICS)\n","            if point3d is not None:\n","                detected_objects.append({'class_name': class_name, '3d_point': point3d, 'bbox': (x1, y1, x2, y2)})\n","                color = (0, 255, 0) if class_name == 'bottle' else (0, 0, 255)\n","                label = f\"{class_name} {conf:.2f} Z:{point3d[2]:.2f}m\"\n","                cv2.rectangle(annotated_frame, (x1, y1), (x2, y2), color, 2)\n","                cv2.putText(annotated_frame, label, (x1, y1 - 10), cv2.FONT_HERSHEY_SIMPLEX, 0.5, color, 2)\n","                cv2.circle(annotated_frame, (u_center, v_center), 5, color, -1)\n","\n","    bottles = [obj for obj in detected_objects if obj['class_name'] == 'bottle']\n","    cups = [obj for obj in detected_objects if obj['class_name'] == 'cup']\n","\n","    if bottles and cups:\n","        bpt, cpt = bottles[0]['3d_point'], cups[0]['3d_point']\n","        dist = calculate_euclidean_distance(bpt, cpt)\n","        if dist is not None:\n","            bx1, by1, bx2, by2 = bottles[0]['bbox']\n","            cx1, cy1, cx2, cy2 = cups[0]['bbox']\n","            b_center = ((bx1 + bx2) // 2, (by1 + by2) // 2)\n","            c_center = ((cx1 + cx2) // 2, (cy1 + cy2) // 2)\n","            mid_x = (b_center[0] + c_center[0]) // 2\n","            mid_y = (b_center[1] + c_center[1]) // 2\n","            cv2.line(annotated_frame, b_center, c_center, (255, 255, 0), 2)\n","            cv2.putText(annotated_frame, f\"Distance: {dist:.2f}m\", (mid_x - 60, mid_y - 10),\n","                        cv2.FONT_HERSHEY_SIMPLEX, 0.6, (255, 255, 0), 2)\n","            print(f\"Frame {frame_count}: Distance between bottle and cup = {dist:.2f}m\")\n","\n","    out_video.write(annotated_frame)\n","    frame_count += 1\n","\n","# --- Cleanup ---\n","cap_rgb.release()\n","cap_depth.release()\n","out_video.release()\n","print(f\"✅ Done! Output saved as: {OUTPUT_VIDEO_FILENAME}\")\n","\n","#  Auto-download\n","files.download(OUTPUT_VIDEO_FILENAME)\n"],"metadata":{"id":"VFXOxigihe33","executionInfo":{"status":"ok","timestamp":1753634359361,"user_tz":-120,"elapsed":257941,"user":{"displayName":"","userId":""}},"outputId":"bda0c7a7-f818-445c-b23c-d6c08194ce1b","colab":{"base_uri":"https://localhost:8080/","height":1000}},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Loading YOLO model: yolov8x.pt...\n","Downloading https://github.com/ultralytics/assets/releases/download/v8.3.0/yolov8x.pt to 'yolov8x.pt'...\n"]},{"output_type":"stream","name":"stderr","text":["100%|██████████| 131M/131M [00:01<00:00, 124MB/s]\n"]},{"output_type":"stream","name":"stdout","text":["Target class IDs: [39, 41]\n","Frame 0: Distance between bottle and cup = 1.98m\n","Frame 1: Distance between bottle and cup = 1.92m\n","Frame 2: Distance between bottle and cup = 1.96m\n","Frame 3: Distance between bottle and cup = 1.99m\n","Frame 4: Distance between bottle and cup = 2.00m\n","Frame 5: Distance between bottle and cup = 1.98m\n","Frame 6: Distance between bottle and cup = 1.90m\n","Frame 7: Distance between bottle and cup = 1.92m\n","Fram